>\r\n\r\n    <div class=\"d-flex justify-content-center meksi10\">\r\n         <div :class=\"cardvue\"  class=\"card-img-play-btn d-flex justify-content-center align-items-center\">\r\n            <img class=\"card-img-play-btn-img\" src=\"../../assets/icon/triangle.png\" alt=\"\">\r\n         </div>\r\n    </div>\r\n   \r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { options } from \"../test.js\";\r\nexport default {\r\n    data() {\r\n        return {\r\n            show:false,\r\n            cardvue:\"cardhidden\",\r\n            movies:{\r\n                backdrop_path:[]\r\n            }\r\n        }\r\n    },\r\n    created() {\r\n        this.getir()\r\n    },\r\n    methods:{\r\n        btnvisible(){this.cardvue=\"cardshow\"},\r\n        btnhidden(){{this.cardvue=\"cardhidden\"}},\r\n        getir(){\r\n        const option=options\r\n        fetch('https://api.themoviedb.org/3/movie/popular', option)\r\n  .then(response => response.json())\r\n\r\n  .then( /*gelen image adresini https://image.tmdb.org/t/p/w500/gelenadres.jpg*/\r\n\r\n        ( gelenObje)=>\r\n        {\r\n\r\n/*\r\n            key olarak page,results,total_pages,total_results anahtarları geliyor\r\n            page  : sayfa \r\n            results : MOVİELERİN BİLGİLERİNİ TUTAN OBJE BULUNUYOR\r\n            total_results: kaç adet movie var\r\n            total_pages: kaç sayfa var\r\n\r\n*/\r\n\r\n        for (const key in gelenObje) {\r\n                if (Object.hasOwnProperty.call(gelenObje, key)) {\r\n                    const element = gelenObje[key];\r\n                    console.log(typeof gelenObje[\"results\"][0]);\r\n\r\n                 \r\n                }\r\n            }\r\n        }\r\n\r\n \r\n    )\r\n  .catch(err => console.error(err));\r\n    }\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n    .card{\r\n        position: relative;\r\n\r\n            }\r\n   .info{\r\n        width: 100%;\r\n        z-index: 12;\r\n        \r\n        color: #fed32f;\r\n\r\n    }\r\n    .info > h6 {\r\n        background-color: #333333;\r\n        padding: 5px;\r\n    }\r\n    .meksi10{margin-bottom: -10px;\r\n        z-index: 13;\r\n    }\r\n    .card-img{\r\n        width: 250px;\r\n        height: 250px;\r\n    }\r\n \r\n    .card-img-play-btn{\r\n        width: 70px;\r\n    height: 70px;\r\n    background-color: white;\r\n    border: #1b1b1b 14px solid;\r\n    border-radius: 50%;\r\n    \r\n   \r\n    \r\n    }\r\n    .card-img-play-btn-img{ \r\n        width: 27px;\r\n    height: 27px;\r\n    border-radius: 50%;\r\n    transform: rotate(90deg);\r\n    padding-bottom: 6px;\r\n    }\r\n    .cardshow{\r\n        opacity: 1;\r\n        transition: 1s;\r\n        cursor: pointer;\r\n\r\n    }\r\n    .cardhidden{opacity: 0;}\r\n    .card-img-play-btn-img:active{ transform: scale(.8);}\r\n\r\n    @media (min-width:375px) and (max-width:425px) \r\n    {\r\n        .card-img{width: 100%;}    \r\n    }\r\n</style>"],"mappings":"AAiBA,SAASA,OAAM,QAAS,YAAY;AACpC,eAAe;EACXC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,IAAI,EAAC,KAAK;MACVC,OAAO,EAAC,YAAY;MACpBC,MAAM,EAAC;QACHC,aAAa,EAAC;MAClB;IACJ;EACJ,CAAC;EACDC,OAAOA,CAAA,EAAG;IACN,IAAI,CAACC,KAAK,CAAC;EACf,CAAC;EACDC,OAAO,EAAC;IACJC,UAAUA,CAAA,EAAE;MAAC,IAAI,CAACN,OAAO,GAAC,UAAU;IAAA,CAAC;IACrCO,SAASA,CAAA,EAAE;MAAC;QAAC,IAAI,CAACP,OAAO,GAAC,YAAY;MAAA;IAAC,CAAC;IACxCI,KAAKA,CAAA,EAAE;MACP,MAAMI,MAAM,GAACX,OAAM;MACnBY,KAAK,CAAC,4CAA4C,EAAED,MAAM,EAC/DE,IAAI,CAACC,QAAO,IAAKA,QAAQ,CAACC,IAAI,CAAC,CAAC,EAEhCF,IAAI,EAAE;;MAECG,SAAS,IACX;QAER;;;;;;;AAOA;;QAEQ,KAAK,MAAMC,GAAE,IAAKD,SAAS,EAAE;UACrB,IAAIE,MAAM,CAACC,cAAc,CAACC,IAAI,CAACJ,SAAS,EAAEC,GAAG,CAAC,EAAE;YAC5C,MAAMI,OAAM,GAAIL,SAAS,CAACC,GAAG,CAAC;YAC9BK,OAAO,CAACC,GAAG,CAAC,OAAOP,SAAS,CAAC,SAAS,CAAC,CAAC,CAAC,CAAC,CAAC;UAG/C;QACJ;MACJ,CAGJ,EACDQ,KAAK,CAACC,GAAE,IAAKH,OAAO,CAACI,KAAK,CAACD,GAAG,CAAC,CAAC;IAC/B;EACA;AACJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}