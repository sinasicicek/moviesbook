(const key in gelenObje) {\n          if (Object.hasOwnProperty.call(gelenObje, key)) {\n            const element = gelenObje[key];\n            console.log(element);\n            if (key === \"results\") {\n              for (let index = 0; index < kac_aadet_gelsin; index++) {\n                this.movies.backdrop_path.push(gelenObje[\"results\"][index].backdrop_path);\n              }\n            } else {}\n            let stoplam = gelenObje[\"total_results\"];\n\n            /*for (const sayi of gelenObje[\"total_results\"]) {\r\n            console.log(gelenObje[\"results\"][sayi].backdrop_path);\r\n            }*/\n          }\n        }\n      }).catch(err => console.error(err));\n      this.card_wiev = true;\n    },\n    movie_sayfa_getir_BTN() {\n      this.movies.page_number = this.movies.page_number + 1;\n      this.popular_movie_getir(20, this.movies.page_number);\n    },\n    türSecimi_Getir_BTN() {\n      this.movies.backdrop_path = [];\n      this.movietürgetir(20, 1, this.movies.movietur, \"en\");\n      console.log(this.movies.konular);\n    },\n    movietürgetir(kac_aadet_gelsin = 100, page_no = 1, movietur = \"movie/popular\", dil) {\n      const option = options;\n      moviegetir(movietur, page_no, dil).then(response => response.json()).then(gelenObje => {\n        for (const key in gelenObje) {\n          if (Object.hasOwnProperty.call(gelenObje, key)) {\n            const element = gelenObje[key];\n            console.log(gelenObje);\n            if (key === \"results\") {\n              for (let index = 0; index < kac_aadet_gelsin; index++) {\n                this.movies.backdrop_path.push(gelenObje[\"results\"][index].backdrop_path);\n              }\n            } else {}\n            let stoplam = gelenObje[\"total_results\"];\n          }\n        }\n      }).catch(err => console.error(err));\n      this.card_wiev = true;\n    },\n    movie_konu_iceriklieri_get() {\n      /*Filmlerim  konularının isimlerini id getirior kullanmak için kullanmalısın\r\n       */\n      moviegetir(\"genre/movie/list\", this.movies.page_number, \"en\").then(response => response.json()).then(gelenObje => {\n        for (const key in gelenObje) {\n          if (Object.hasOwnProperty.call(gelenObje, key)) {\n            const element = gelenObje[key];\n            element.forEach(elaman => {\n              this.movies.konular.id.push(elaman.id);\n              this.movies.konular.isim.push(elaman.name);\n            });\n          }\n        }\n        console.log(this.movies.konular.isim);\n      }).catch(err => console.error(err));\n    }\n  }\n};","map":{"version":3,"names":["card","navbar","auth","onAuthStateChanged","options","moviegetir","turSelection","checkbox","data","card_wiev","tablostate","checkbox_gelen_konu_id","movies","page_number","backdrop_path","movietur","konular","id","isim","emits","created","popular_movie_getir","movie_konu_iceriklieri_get","beforeCreate","user","$router","push","components","methods","secilenTür","glen","konu_secimi","gelen_konu","kac_aadet_gelsin","page_no","option","then","response","json","gelenObje","key","Object","hasOwnProperty","call","element","console","log","index","stoplam","catch","err","error","movie_sayfa_getir_BTN","türSecimi_Getir_BTN","movietürgetir","dil","forEach","elaman","name"],"sources":["/home/sinasi/Masaüstü/Geliştirme/MoviesBook/movie_books/src/components/homes/Home.vue"],"sourcesContent":["<template>\r\n    <navbar />\r\n    <div class=\"Home p-5\">\r\n        <div>\r\n            <turSelection @secilenTur=\"secilenTür($event)\"></turSelection>\r\n            <button @click=\"türSecimi_Getir_BTN()\">getir</button>\r\n<!-- \r\n                <div>\r\n                <checkbox @secilenKonu=\"konu_secimi($event)\" :konularname=\"movies.konular.isim\" :konularid=\"movies.konular.id\" ></checkbox>\r\n                <button @click=\"konuSecimi_Getir_BTN()\">Filtrele</button>\r\n            </div>\r\n-->\r\n\r\n       \r\n</div>\r\n        <div class=\"container\">\r\n            <ul class=\"liste-ul\">\r\n                <li class=\"liste-li\">Populer Filmler</li>\r\n                <li class=\"liste-li\">Keşfet</li>\r\n                <li class=\"liste-li\">Haftanın Trend Filmleri</li>\r\n                \r\n            </ul>\r\n            <br>\r\n            <div class=\"liste-hr\"></div>\r\n        </div>\r\n        <br>\r\n        <div id=\"tablo\"\r\n            v-if=\"tablostate\"\r\n            class=\"container d-flex justify-content-center align-items-center flex-wrap Home-container\">\r\n          \r\n            <card\r\n                class=\"m-2\"\r\n                v-for=\"(item, index) in movies.backdrop_path\"\r\n                :imgsrc=\"item\"\r\n                :key=\"index\"\r\n                :id=\"index\"\r\n            ></card>\r\n        </div>\r\n        <button class=\"btn btn-danger\" @click=\"movie_sayfa_getir_BTN()\">\r\n            Daha fazla getir\r\n        </button>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport card from \"../homes/movieimgcard.vue\";\r\nimport navbar from \"../../components/navbar/navbarliste.vue\";\r\nimport { auth } from \"../../components/firebaseconf.js\";\r\nimport { onAuthStateChanged } from \"firebase/auth\";\r\nimport { options, moviegetir } from \"../test.js\";\r\nimport turSelection from \"../selectionitem/sele.vue\";\r\n\r\nimport checkbox from \"../checkboxitem/chec.vue\";\r\n\r\nexport default {\r\n    data() {\r\n        return {\r\n            card_wiev: true,\r\n            tablostate: true,\r\n            checkbox_gelen_konu_id:\"\",\r\n            movies: {\r\n                page_number: 1,\r\n                backdrop_path: [],\r\n                movietur: \"\",\r\n                konular:{\r\n                    id:[],\r\n                    isim:[]\r\n                }\r\n            },\r\n        };\r\n    },\r\n    emits: [\"secilenTur\"],\r\n    created() {\r\n        this.popular_movie_getir();\r\n        this.movie_konu_iceriklieri_get()\r\n    },\r\n    beforeCreate() {\r\n        onAuthStateChanged(auth, (user) => {\r\n            if (user === null) {\r\n                this.$router.push(\"/\");\r\n            }\r\n        });\r\n    },\r\n    components: { card, navbar, turSelection,checkbox },\r\n    methods: {\r\n        secilenTür(glen) {\r\n            this.movies.movietur = glen;\r\n        },\r\n        konu_secimi(gelen_konu){this.checkbox_gelen_konu_id=gelen_konu},\r\n        popular_movie_getir(kac_aadet_gelsin = 100, page_no = 1, movietur = \"movie/popular\") {\r\n            const option = options;\r\n            //fetch(\"https://api.themoviedb.org/3/movie/popular?page=\"+page_no+\"&language=tr-tr\", option)\r\n            moviegetir(movietur, page_no, \"tr\")\r\n                .then((response) => response.json())\r\n\r\n                .then(\r\n                    /*gelen image adresini https://image.tmdb.org/t/p/w500/gelenadres.jpg*/\r\n\r\n                    (gelenObje) => {\r\n                        /*\r\n            key olarak page,results,total_pages,total_results anahtarları geliyor\r\n            page  : sayfa \r\n            results : MOVİELERİN BİLGİLERİNİ TUTAN OBJE BULUNUYOR\r\n            total_results: kaç adet movie var\r\n            total_pages: kaç sayfa var\r\n\r\n*/\r\n\r\n                        for (const key in gelenObje) {\r\n                            if (Object.hasOwnProperty.call(gelenObje, key)) {\r\n                                const element = gelenObje[key];\r\n\r\n                                console.log(element);\r\n                                if (key === \"results\") {\r\n                                    for (\r\n                                        let index = 0;\r\n                                        index < kac_aadet_gelsin;\r\n                                        index++\r\n                                    ) {\r\n                                        this.movies.backdrop_path.push(\r\n                                            gelenObje[\"results\"][index]\r\n                                                .backdrop_path\r\n                                        );\r\n                                    }\r\n                                } else {\r\n                                }\r\n\r\n                                let stoplam = gelenObje[\"total_results\"];\r\n\r\n                                /*for (const sayi of gelenObje[\"total_results\"]) {\r\n                    console.log(gelenObje[\"results\"][sayi].backdrop_path);\r\n                    }*/\r\n                            }\r\n                        }\r\n                    }\r\n                )\r\n                .catch((err) => console.error(err));\r\n            this.card_wiev = true;\r\n        },\r\n        movie_sayfa_getir_BTN() {\r\n            this.movies.page_number = this.movies.page_number + 1;\r\n            this.popular_movie_getir(20, this.movies.page_number);\r\n        },\r\n        türSecimi_Getir_BTN() {\r\n            \r\n            this.movies.backdrop_path = [];\r\n            this.movietürgetir(20, 1, this.movies.movietur,\"en\");\r\n            console.log(this.movies.konular);\r\n\r\n        },\r\n        movietürgetir(\r\n            kac_aadet_gelsin = 100,\r\n            page_no = 1,\r\n            movietur = \"movie/popular\",\r\n            dil) {\r\n            const option = options;\r\n            moviegetir(movietur, page_no, dil)\r\n                .then((response) => response.json())\r\n\r\n                .then((gelenObje) => {\r\n\r\n\r\n                    for (const key in gelenObje) {\r\n                        if (Object.hasOwnProperty.call(gelenObje, key)) {\r\n                            const element = gelenObje[key];\r\n\r\n                            console.log(gelenObje);\r\n                            if (key === \"results\") {\r\n                                for (\r\n                                    let index = 0;\r\n                                    index < kac_aadet_gelsin;\r\n                                    index++\r\n                                ) {\r\n                                    this.movies.backdrop_path.push(\r\n                                        gelenObje[\"results\"][index]\r\n                                            .backdrop_path\r\n                                    );\r\n                                }\r\n                            } else {\r\n                            }\r\n\r\n                            let stoplam = gelenObje[\"total_results\"];\r\n                        }\r\n                    }\r\n                })\r\n                .catch((err) => console.error(err));\r\n            this.card_wiev = true;\r\n        },\r\n        movie_konu_iceriklieri_get(){\r\n            /*Filmlerim  konularının isimlerini id getirior kullanmak için kullanmalısın\r\n             */\r\n            moviegetir(\"genre/movie/list\", this.movies.page_number, \"en\")\r\n                .then((response) => response.json())\r\n\r\n                .then(\r\n                    (gelenObje) => {\r\n                for (const key in gelenObje) {\r\n                            if (Object.hasOwnProperty.call(gelenObje, key)) {\r\n                                const element = gelenObje[key];\r\n\r\n                                element.forEach(elaman => {\r\n                              \r\n                                this.movies.konular.id.push(elaman.id)\r\n                                this.movies.konular.isim.push(elaman.name)\r\n\r\n                                 });\r\n                            }\r\n                        }\r\n                        console.log(this.movies.konular.isim);\r\n                    }\r\n                )\r\n                .catch((err) => console.error(err));\r\n        },\r\n\r\n    },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n    ul {\r\n       margin: 0px; padding: 0px;\r\n        display: flex;\r\n        justify-content: